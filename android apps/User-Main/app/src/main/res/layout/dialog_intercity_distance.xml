<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/transparent_70"
        android:clickable="true"
        android:gravity="bottom"
        android:orientation="vertical"
        android:focusable="true">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_minus5sdp"
            android:background="@drawable/card_view_23_white_line_flat_1"
            android:gravity="center"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <com.view.MTextView
                    android:id="@+id/locationErrorTitleTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="?attr/actionBarSize"
                    android:layout_centerInParent="true"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:textColor="@color/text23Pro_Dark"
                    android:textSize="@dimen/txt_size_14"
                    app:customTypeFace="@string/system_bold"
                    tools:text="Location Error" />


                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/closeDialogImageView"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/_2sdp"
                    android:padding="@dimen/_4sdp"
                    app:srcCompat="@drawable/ic_close_round" />
            </RelativeLayout>

            <View
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_1sdp"
                android:background="@color/view23ProBG" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:padding="@dimen/_10sdp">

                <com.view.MTextView
                    android:id="@+id/noServiceWithingCityTextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center_vertical"
                    android:clickable="false"
                    android:textAlignment="viewStart"
                    android:textColor="@color/textSub23Pro_gray"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="normal"
                    app:customTypeFace="@string/system_regular"
                    tools:text="Location Error" />

                <include
                    android:id="@+id/chooseOtherTripBtn"
                    layout="@layout/btn_type_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_15sdp" />

                <include
                    android:id="@+id/editLocationBtn"
                    layout="@layout/btn_type_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:layout_marginBottom="@dimen/_5sdp" />

            </LinearLayout>


        </LinearLayout>
    </LinearLayout>
</layout>